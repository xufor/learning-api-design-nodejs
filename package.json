{
  "name": "api-design-v5",
  "version": "1.0.0",
  "description": "Habit Tracker API for Node.js course",
  "main": "src/index.ts",
  "type": "module",
  "engines": {
    "node": ">=24.3.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development node --watch src/index.ts",
    "start": "cross-env NODE_ENV=production node src/index.ts",
    "test": "cross-env NODE_ENV=test vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:seed": "node src/db/seed.ts"
  },
  "keywords": [
    "habit-tracker",
    "api",
    "nodejs",
    "express",
    "drizzle"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@epic-web/remember": "^1.1.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "custom-env": "^2.0.6",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.44.4",
    "drizzle-zod": "^0.8.3",
    "express": "^5.1.0",
    "helmet": "^7.1.0",
    "jose": "^6.0.12",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.5",
    "@types/supertest": "^6.0.2",
    "cross-env": "^10.0.0",
    "drizzle-kit": "^0.31.4",
    "supertest": "^6.3.4",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
